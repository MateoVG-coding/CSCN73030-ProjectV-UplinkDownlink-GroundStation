{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/**.sln"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
			"_site"
          ],
        "cwd": ".."
        }
      ]
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "../docs/api/**.yml",
          "../docs/api/index.md"
        ]
      },
      {
        "files": [
          "../docs/articles/**.md",
          "../docs/articles/**/toc.yml",
          "../docs/toc.yml",
          "../docs/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "../docs/api",
    "output": "../docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
