{
  "metadata": [
    {
      "src": "../Project 5/**/*.csproj", // Adjust the path to match your project location
      "dest": "src",
      "isDependency": true,
      "fileMetadata": [
        {
          "src": "project.json",
          "key": "type",
          "value": "project"
        }
      ]
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "src/**/*.cs",
          "src/**/*.vb"
        ],
        "src": "src",
        "dest": "api"
      },
      {
        "files": [
          "docs/**/*.md"
        ],
        "src": "docs",
        "dest": "articles"
      }
    ],
    "dest": "../docs/_site",
    "globalMetadata": {
      "_appTitle": "Ground: Uplink and Downlink",
      "_disableContribution": true
    },
    "fileMetadata": [
      {
        "src": "**/*.md",
        "dest": "",
        "key": "is_index",
        "value": true
      }
    ],
    "template": [
      "default"
    ]
  },
  "template": {
    "name": "default",
    "fallbackFolders": [
      "_shared"
    ]
  },
  "pdf": {
    "strategy": "strategy",
    "pdfDocsets": [
      {
        "src": "docfx_project",
        "dest": "apidoc"
      }
    ],
    "dest": "pdf"
  }
}
