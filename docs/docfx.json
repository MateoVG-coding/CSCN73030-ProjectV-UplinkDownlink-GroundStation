{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/**.sln"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
          ],
        "src":"../.."
        }

      ]
    }
  ],
  "build": {
    "content": [
      { "files": "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md", "exclude": "**/include/**" }
    ],
    "resource": [
      { "files": "images/**"}
    ],
    "dest": "api",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
