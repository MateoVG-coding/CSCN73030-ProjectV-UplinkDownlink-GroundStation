{
  "metadata": [
    {
      "src": "../Project 5/**/*.csproj", // Adjust the path to match your project location
      "dest": "src",
      "isDependency": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "src/**/*.cs"
        ],
        "src": "src",
        "dest": "api"
      },
      {
        "files": [
          "docs/**/*.md"
        ],
        "src": "docs",
        "dest": "articles"
      }
    ],
    "dest": "../docs/_site",
    "globalMetadata": {
      "_appTitle": "Ground: Uplink and Downlink",
      "_disableContribution": true
    }
}
